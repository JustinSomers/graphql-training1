{
  "name": "supergraph-router",
  "version": "1.1.18",
  "description": "",
  "main": "gateway.ts",
  "scripts": {
    "start": "ts-node-dev gateway.ts",
    "generate-typings": "graphql-codegen",
    "build": "npm run-script generate-typings && tsc && npm run-script build-schema",
    "build-schema": "find ./src -type f -name \\*.graphql -not -path '*/__tests__/*' -exec cat {} + > build/schema.graphql"
  },
  "dependencies": {
    "@apollo/gateway": "^2.1.1",
    "@graphql-tools/graphql-file-loader": "^7.5.3",
    "@graphql-tools/load": "^7.7.5",
    "@graphql-tools/schema": "^9.0.2",
    "@types/node": "^18.7.14",
    "apollo-server": "3.9.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "graphql": "^16.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  },
  "keywords": [],
  "author": ""
}
